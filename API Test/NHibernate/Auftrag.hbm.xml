<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="API Test"
                   namespace="Models">

	<class name="Auftrag" table="Auftraege" lazy="false">
